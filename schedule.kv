<ScheduleScreen>:
    BoxLayout:
        orientation: 'vertical'
        padding: 20
        spacing: 10

        Label:
            text: "Rencana Liburan"
            font_size: 24
            bold: True

        Label:
            id: schedule_info
            text: "DAY-1\n- Candi Prambanan\n03/Des/2024\n10.30 WIB\nRp20.000"
            font_size: 18
            halign: 'center'

        Button:
            text: "DONE"
            size_hint_y: None
            height: 50
            on_press: root.manager.current = 'user'
